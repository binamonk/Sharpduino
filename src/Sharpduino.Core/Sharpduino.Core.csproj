<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <PackageId>Sharpduino.Core</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <Authors>Dan Miranda</Authors>
    <Description>Library to control Arduino compatible devices with Firmata protocol.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First alpha release.</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Binamedia.</Copyright>
    <PackageTags>arduino firmata library</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.1\Sharpduino.Core.xml</DocumentationFile>
  </PropertyGroup>

</Project>